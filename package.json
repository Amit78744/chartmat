		"chartjs-plugin-datalabels": "^2.0.0",
		"color": "^4.2.3",
		"date-fns": "^2.20.0",
		"dayjs": "^1.11.7",
		"dompurify": "^2.2.9",
		"flatpickr": "^4.6.9",
