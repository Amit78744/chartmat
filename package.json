{
	"name": "chartmat",
	"version": "1.0.0",
	"description": "chartmat",
	"author": "",
	"private": true,
	"engines": {
		"node": "18.x"
	},
	"engineStrict": true,
	"scripts": {
		"dev": "npm run dev_backend & npm run dev_frontend",
		"dev_win": "npm run dev_backend && npm run dev_frontend",
		"dev_backend": "nodemon --watch api -r dotenv/config ./api/serverless.js",
		"dev_frontend": "nuxt dev -o --port 3000",
		"build": "nuxt build",
		"start": "nuxt start",
		"analyze": "nuxt build --analyze",
		"generate": "nuxt generate --modern --fail-on-error",
		"kill": "killall -9 node",
		"netlify-dev": "netlify dev",
		"load-coupon": "node -r dotenv/config load_coupons.js",
		"update-market": "node -r dotenv/config ./api/cli/update_marketplace.js",
		"ltd-user": "node -r dotenv/config ./api/cli/ltd_users.js",
		"go-live": "npx localtunnel --port 3000 --subdomain chartmat",
		"reboot": "rm -r node_modules && rm -r .nuxt && rm package-lock.json && npm i"
	},
	"dependencies": {
		"@eastdesire/jscolor": "^2.4.6",
